    "expo-standard-web-crypto": "~2.1.4",
    "expo-status-bar": "~2.2.3",
    "react": "catalog:react19",
    "react-native": "0.80.0",
    "react-native-webrtc": "^124.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@rtco/eslint-config": "workspace:*",
    "@rtco/prettier-config": "workspace:*",
    "@rtco/typescript-config": "workspace:*",
